    "jest-canvas-mock": "^1.1.0",
    "prettier": "^1.7.0",
    "react-hot-loader": "^4.3.4",
    "regenerator-runtime": "^0.12.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.0.0",
    "tslint": "^5.11.0",
